<?xml version="1.0"?>

<copperspec id="CompositionTestHost" type="LALR1" version="1.1">
   <term id="spaces">
      <classes>
         <termclass id="whitespace"/>
      </classes>
      <regex><kleenestar><charset><loosechar>&#x20;</loosechar></charset>	</kleenestar></regex>
      <dominates>
	<term id="tabs"/>
	<term id="newlines"/>
      </dominates>
   </term>
   <term id="tabs">
      <classes>
         <termclass id="whitespace"/>
      </classes>
      <regex><kleenestar><charset><loosechar>&#09;</loosechar></charset></kleenestar></regex>
   </term>
   <term id="newlines">
      <classes>
         <termclass id="whitespace"/>
      </classes>
      <regex><kleenestar><charset><loosechar>&#10;</loosechar></charset></kleenestar></regex>
   </term>
   <term id="+">
      <classes>
         <termclass id="main"/>
      </classes>
      <regex><string>+</string></regex>
      <operator>
        <precedence>0</precedence>
        <associativity>left</associativity>
        <opclass id="arith"/>
      </operator>
   </term>
   <term id="-">
      <classes>
         <termclass id="main"/>
      </classes>
      <regex><string>-</string></regex>
      <operator>
        <precedence>0</precedence>
        <associativity>left</associativity>
        <opclass id="arith"/>
      </operator>
   </term>
   <term id="*">
      <classes>
         <termclass id="main"/>
      </classes>
      <regex><string>*</string></regex>
      <operator>
        <precedence>1</precedence>
        <associativity>left</associativity>
        <opclass id="arith"/>
      </operator>
   </term>
   <term id="/">
      <classes>
         <termclass id="main"/>
      </classes>
      <regex><string>/</string></regex>
      <operator>
        <precedence>1</precedence>
        <associativity>left</associativity>
        <opclass id="arith"/>
      </operator>
   </term>
   <term id="(">
      <classes>
         <termclass id="main"/>
      </classes>
      <regex><string>(</string></regex>
   </term>
   <term id=")">
      <classes>
         <termclass id="main"/>
      </classes>
      <regex><string>)</string></regex>
   </term>
   <term id="digit">
      <classes>
         <termclass id="main"/>
      </classes>
      <regex><string>0</string></regex>
   </term>
   <term id="id">
      <classes>
	<termclass id="main"/>
      </classes>
      <regex><string>a</string></regex>
   </term>

   <nonterm id="E"/>

   <start>
     <nonterm id="E"/>
     <layout>
       <term id="spaces"/>
       <term id="tabs"/>
       <term id="newlines"/>
     </layout>
   </start>

   <prod id="EplusE">
     <class>arith</class>
     <precedence>1</precedence>
     <lhs><nonterm id="E"/></lhs>
     <rhs>
        <nonterm id="E"/>
	<term id="+"/>
	<nonterm id="E"/>
     </rhs>
     <layout><term id="spaces"/></layout>
   </prod>
   <prod id="EminusE" class="arith" precedence="1">
     <lhs><nonterm id="E"/></lhs>
     <rhs>
        <nonterm id="E"/>
	<term id="-"/>
	<nonterm id="E"/>
     </rhs>
     <layout><term id="spaces"/></layout>
   </prod>
   <prod id="EtimesE">
     <class>arith</class>
     <precedence>1</precedence>
     <lhs><nonterm id="E"/></lhs>
     <rhs>
        <nonterm id="E"/>
	<term id="*"/>
	<nonterm id="E"/>
     </rhs>
     <layout><term id="spaces"/></layout>
   </prod>
   <prod id="EdivE">
     <class>arith</class>
     <precedence>1</precedence>
     <lhs><nonterm id="E"/></lhs>
     <rhs>
        <nonterm id="E"/>
	<term id="/"/>
	<nonterm id="E"/>
     </rhs>
     <layout><term id="spaces"/></layout>
   </prod>
   <prod id="parenE">
     <class>arith</class>
     <precedence>1</precedence>
     <lhs><nonterm id="E"/></lhs>
     <rhs>
        <term id="("/>
        <nonterm id="E"/>
        <term id=")"/>
     </rhs>
     <layout><term id="spaces"/></layout>
   </prod>
   <prod id="Edigit">
     <class>atomic</class>
     <precedence>1</precedence>
     <lhs><nonterm id="E"/></lhs>
     <rhs>
	<term id="digit"/>
     </rhs>
     <layout><term id="spaces"/></layout>
   </prod>
   <prod id="Eid">
     <class>atomic</class>
     <precedence>1</precedence>
     <lhs><nonterm id="E"/></lhs>
     <rhs>
	<term id="id"/>
     </rhs>
     <layout><term id="spaces"/></layout>
   </prod>
   
</copperspec>
